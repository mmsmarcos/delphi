  object qUsuariosImpressao: TFDQuery
    Connection = DM.fdConexao
    SQL.Strings = (
      'SELECT t.USERNAME,'
      '       t.NOME,'
      '       t.EMAIL,'
      
        '       CASE WHEN (t.ATIVO = 1) THEN '#39'Ativo'#39' ELSE '#39'Inativo'#39' END A' +
        'S ATIVO,'
      '       t.ULTLOGIN'
      '  FROM TBUSUARIO t')
