  object qCadastraVeiculo: TFDQuery
    Connection = DM.fdConexao
    SQL.Strings = (
      'select * from tbveiculos where idveiculo = -1')
    Left = 288
    Top = 360
  end
  object qTipoRodado: TFDQuery
    Connection = DM.fdConexao
    SQL.Strings = (
      'SELECT IDEMPRESA, IDRODADO, DESCRICAO '
      '  FROM TBTIPORODADO'
      ' WHERE IDEMPRESA = :IDEMPRESA')
    Left = 400
    Top = 360
    ParamData = <
      item
        Name = 'IDEMPRESA'
        DataType = ftInteger
        ParamType = ptInput
      end>
    object qTipoRodadoIDEMPRESA: TIntegerField
      FieldName = 'IDEMPRESA'
      Origin = 'IDEMPRESA'
      Required = True
    end
    object qTipoRodadoIDRODADO: TIntegerField
      FieldName = 'IDRODADO'
      Origin = 'IDRODADO'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qTipoRodadoDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Origin = 'DESCRICAO'
      Required = True
      Size = 100
    end
  end
  object dsTipoRodado: TDataSource
    DataSet = qTipoRodado
    Left = 504
    Top = 360
  end
  object qVeiculoRodado: TFDQuery
    Connection = DM.fdConexao
    SQL.Strings = (
      'SELECT t.IDVEICULO , t2.IDRODADO, t2.DESCRICAO '
      
        '  FROM TBVEICULOS t JOIN TBTIPORODADO t2 ON t.IDEMPRESA = T2.IDE' +
        'MPRESA AND t.IDTIPORODADO = T2.IDRODADO '
      ' WHERE t.IDEMPRESA = :IDEMPRESA '
      '   AND t.IDVEICULO = :IDVEICULO')
    Left = 472
    Top = 432
    ParamData = <
      item
        Name = 'IDEMPRESA'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'IDVEICULO'
        DataType = ftInteger
        ParamType = ptInput
      end>
    object qVeiculoRodadoIDVEICULO: TIntegerField
      FieldName = 'IDVEICULO'
      Origin = 'IDVEICULO'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qVeiculoRodadoIDRODADO: TIntegerField
      AutoGenerateValue = arDefault
      FieldName = 'IDRODADO'
      Origin = 'IDRODADO'
      ProviderFlags = []
      ReadOnly = True
    end
    object qVeiculoRodadoDESCRICAO: TStringField
      AutoGenerateValue = arDefault
      FieldName = 'DESCRICAO'
      Origin = 'DESCRICAO'
      ProviderFlags = []
      ReadOnly = True
      Size = 100
    end
  end
